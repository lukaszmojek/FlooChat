<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ChatController">
    <children>
        <Pane fx:id="mainAppPane" prefHeight="800.0" prefWidth="800.0" style="-fx-background-color: yellow;">
            <children>
                <Button fx:id="backButton" layoutX="11.0" layoutY="756.0" mnemonicParsing="false" onAction="#backButtonClick" prefHeight="30.0" prefWidth="170.0" style="-fx-background-color: red;" text="Powrót" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="14.0" />
                    </font>
                </Button>
                <Label fx:id="welcomeLabel" layoutX="11.0" layoutY="14.0" prefHeight="43.0" prefWidth="614.0" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="29.0" />
                    </font>
                </Label>
                <TextArea fx:id="messageArea" editable="false" layoutX="34.0" layoutY="153.0" prefHeight="506.0" prefWidth="732.0" />
                <TextField fx:id="messageField" layoutX="31.0" layoutY="690.0" prefHeight="25.0" prefWidth="667.0" promptText="wybierz znajomego i wprowadź wiadomość" />
                <Button fx:id="messageButton" layoutX="711.0" layoutY="690.0" mnemonicParsing="false" onAction="#messageButtonClick" style="-fx-background-color: red;" text="Wyslij" textFill="WHITE">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font></Button>
                <Label fx:id="friendEmailLabel" layoutX="36.0" layoutY="111.0" prefHeight="35.0" prefWidth="634.0" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font></Label>
            </children>
        </Pane>
    </children>
</AnchorPane>
