<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="changePasswordMainPane" prefHeight="800.0" prefWidth="800.0" stylesheets="@application.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ChangePasswordController">
   <children>
       <Pane layoutX="144.0" layoutY="177.0" prefHeight="365.0" prefWidth="513.0">
           <children>
               <PasswordField fx:id="newPasswordField" layoutX="132.0" layoutY="141.0" prefHeight="35.0" prefWidth="250.0" promptText="new password" style="-fx-background-color: #ffffff;">
                   <font>
                       <Font size="14.0" />
                   </font></PasswordField>
               <PasswordField fx:id="confirmPasswordField" layoutX="132.0" layoutY="218.0" prefHeight="35.0" prefWidth="250.0" promptText="confirm password" style="-fx-background-color: white;">
                   <font>
                       <Font size="14.0" />
                   </font></PasswordField>
               <PasswordField fx:id="actualPasswordField" layoutX="132.0" layoutY="70.0" prefHeight="35.0" prefWidth="250.0" promptText="password" style="-fx-background-color: #ffffff;">
                   <font>
                       <Font size="14.0" />
                   </font></PasswordField>
               <Button fx:id="changePasswordButton" layoutX="296.0" layoutY="283.0" mnemonicParsing="false" onAction="#changePasswordButtonClick" prefHeight="35.0" prefWidth="86.0" style="-fx-background-color: red;" text="Zmień hasło" textFill="WHITE">
                   <font>
                       <Font name="System Bold" size="12.0" />
                   </font></Button>
               <Label layoutX="131.0" layoutY="40.0" text="Aktualne hasło" textFill="WHITE">
                   <font>
                       <Font name="System Bold" size="20.0" />
                   </font>
               </Label>
               <Label layoutX="131.0" layoutY="188.0" text="Potwierdź hasło" textFill="WHITE">
                   <font>
                       <Font name="System Bold" size="20.0" />
                   </font>
               </Label>
               <Label layoutX="96.0" layoutY="227.0" />
               <Label fx:id="tablet_label" layoutX="96.0" layoutY="227.0" text=" " textFill="WHITE" />
               <Label layoutX="131.0" layoutY="111.0" text="Nowe hasło" textFill="WHITE">
                   <font>
                       <Font name="System Bold" size="20.0" />
                   </font>
               </Label>
           </children>
       </Pane>
      <Button fx:id="backButton" layoutX="37.0" layoutY="738.0" mnemonicParsing="false" onAction="#backButtonClick" prefHeight="35.0" prefWidth="86.0" style="-fx-background-color: red;" text="Powrót" textFill="WHITE">
          <font>
              <Font name="System Bold" size="12.0" />
          </font></Button>
   </children>
</AnchorPane>
